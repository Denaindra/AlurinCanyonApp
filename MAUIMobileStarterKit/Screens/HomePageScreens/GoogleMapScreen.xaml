<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:maps="clr-namespace:Maui.GoogleMaps;assembly=Maui.GoogleMaps"
             x:Class="MAUIMobileStarterKit.Screens.HomePageScreens.GoogleMapScreen">
    <VerticalStackLayout VerticalOptions="FillAndExpand" >
        <maps:Map MyLocationEnabled="True"  VerticalOptions="FillAndExpand" x:Name="mapview" />
    </VerticalStackLayout>
</ContentView>
